FROM continuumio/miniconda3
RUN conda create --name my_venediktova_2
RUN git clone https://github.com/geopandas/geopandas.git \
    && cd geopandas \
    && pip install .
RUN git clone https://github.com/mwaskom/seaborn.git \
    && cd seaborn \
    && pip install .
RUN git clone https://github.com/nltk/nltk.git \
    && cd nltk \
    && pip install .
RUN git clone https://github.com/dbrgn/star.git \
    && cd star \
    && pip install star